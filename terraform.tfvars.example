location       = "usgovarizona"
environment    = "dev"
cluster_name   = "datasci"

subnet_cidr = "10.0.11.0/24"
subnet_start_address = "10.0.11.1"
subnet_end_address = "10.0.11.254"
grafana_admin_user = "admin"

default_tags   = {
  Department  = "Monkey"
  PoC         = "LiveStream"
  System      = "DataSci"
  SubSystem   = "Grafana"
  Role        = "FrontEnd"
  Environment = "DEV"
}

topics = ["lte_message", "umts_message", "cdma_message", "gsm_message", "80211_beacon_message"]
mqtt_users  = ["dino", "christian", "les", "steve", "shared"]

consumers =  [
  {topic: "lte_message", message_type: "LteRecord", message_version: "~=0.1.0", buffer_size: 1, log_level: "DEBUG"},
    {topic: "umts_message", message_type: "UmtsRecord", message_version: "~=0.1.0", buffer_size: 1, log_level: "DEBUG"},
    {topic: "cdma_message", message_type: "CdmaRecord", message_version: "~=0.1.0", buffer_size: 1, log_level: "DEBUG"},
    {topic: "gsm_message", message_type: "GsmRecord", message_version: "~=0.1.0", buffer_size: 1, log_level: "DEBUG"},
    {topic: "80211_beacon_message", message_type: "WifiBeaconRecord", message_version: "~=0.1.0", buffer_size: 1, log_level: "DEBUG"}
]

eventhub_keys = ["1", "2", "3", "4", "5"]
eventhub_shared_access_policies = ["1a", "2b", "3c", "4d", "5f"]
eventhub_namespace = "asdf"
resource_group_name = "rg-datasci-dev"
virtual_network_name = "vnet-datasci-dev"
